[Unit]
Description=NextEPC P-GWU Daemon
After=networking.service

[Service]
Type=simple

User=nextepc
Group=nextepc

RuntimeDirectory=open5gc-upfd
PIDFile=@LIB_DIR@/run/open5gc-upfd/pid

Restart=always
ExecStart=@BIN_DIR@/open5gc-upfd -f @SYSCONF_DIR@/open5gc/upf.conf
RestartSec=2
RestartPreventExitStatus=1

[Install]
WantedBy=multi-user.target
